<!doctype html>
<html>
  <head>
    <title>Welcome to <%=recipeAppName%></title>
    <link rel="stylesheet"  type="text/css" href="main.css" />
  </head>
  <body>
    <h1><%=recipeAppName%> Shop</h1>
    <h2>Here are the users that are registered with us: </h2>
    <table>
      <tr>
        <th>Username</th>
      </tr>
      <% availableUsers.forEach(function(user) { %>
        <tr>
          <td><%= user.username %></td>
        </tr>
      <% }) %>
    </table>
    <p><a href="./">Back to home</a></p>
  </body>
</html>